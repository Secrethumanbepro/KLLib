tab1:Button("Collect All Droped Keys",function()
	local sound = Instance.new("Sound")
	sound.SoundId = "rbxassetid://1673280232"
	sound.Parent = game:GetService("SoundService")
	sound:Play()

	for i,v in pairs(game.Workspace.Particles:GetDescendants()) do
	if v.Name == "Key" or v.Name == "Red Key" or v.Name == "Green Key" then
	speaker.Character:FindFirstChildOfClass("Humanoid").Sit = true
	local X = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.X
	local Y = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.Y
	local Z = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.Z

	game.Players.localPlayer.Character.HumanoidRootPart.CFrame = v.CFrame
	wait(0.2)
	game.Players.localPlayer.Character:FindFirstChildOfClass("Humanoid").Jump = true

	game.Players.localPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(X, Y, Z)


	end
	end
	end)
tab1:Button("Search For Someone Who Has A Key",function()
	local sound = Instance.new("Sound")
	sound.SoundId = "rbxassetid://1673280232"
	sound.Parent = game:GetService("SoundService")
	sound:Play()

	for index, child in pairs(game.Players:GetChildren()) do
	if child.Backpack:FindFirstChild("Green Key") or child.Character:FindFirstChild("Green Key") then
	game.StarterGui:SetCore("SendNotification", {
		Title = "Green Key Detected"; -- the title (ofc)
		Text = "Hello There I Have A Green Key And My Name is "..child.Name; -- what the text says (ofc)
		Icon = game.Players:GetUserThumbnailAsync(child.UserId, Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size420x420); -- the image if u want.
		Duration = 7; -- how long the notification should in secounds
	})

	elseif child.Backpack:FindFirstChild("Red Key") or child.Character:FindFirstChild("Red Key") then

	game.StarterGui:SetCore("SendNotification", {
		Title = "Red Key Detected"; -- the title (ofc)
		Text = "Hello There I Have A Red Key And My Name is "..child.Name; -- what the text says (ofc)
		Icon = game.Players:GetUserThumbnailAsync(child.UserId, Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size420x420); -- the image if u want.
		Duration = 7; -- how long the notification should in secounds
	})

	elseif child.Backpack:FindFirstChild("Key") or child.Character:FindFirstChild("Key") then

	game.StarterGui:SetCore("SendNotification", {
		Title = "White Key Detected"; -- the title (ofc)
		Text = "Hello There I Have A White Key And My Name is "..child.Name; -- what the text says (ofc)
		Icon = game.Players:GetUserThumbnailAsync(child.UserId, Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size420x420); -- the image if u want.
		Duration = 7; -- how long the notification should in secounds
	})
	end end end)
