tab1:Button("Anti Stun",function(state)
	local StarterGui = game:GetService("StarterGui")

	local Callback = Instance.new("BindableFunction")

	local player = game.Players.LocalPlayer


	function Callback.OnInvoke(response)

	if response == "ON" then
	local sound = Instance.new("Sound")
	sound.SoundId = "rbxassetid://1673280232"
	sound.Parent = game:GetService("SoundService")
	sound:Play()

	if no4 then
	no4:Disconnect()
	end
	local function no4Func()
	if game.Players.localPlayer.Character.Stats.Downed.Value == true then
	game.Players.localPlayer.Character.Stats.Downed.Value = false
	game.Players.localPlayer.Character.Stats.Ragdoll.Value = false
	game.Players.localPlayer.Character.Humanoid.WalkSpeed = 18
	game.Players.localPlayer.PlayerGui.TouchGui.TouchControlFrame.JumpButton.Visible = true
	game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
	elseif game.Players.localPlayer.Character.Stats.Ragdoll.Value == true then
	game.Players.localPlayer.Character.Stats.Ragdoll.Value = false
	end
	end
	no4 = game:GetService("RunService").RenderStepped:Connect(no4Func)
	elseif response == "OFF" then

	local sound = Instance.new("Sound")
	sound.SoundId = "rbxassetid://1673280232"
	sound.Parent = game:GetService("SoundService")
	sound:Play()

	if no4 then
	no4:Disconnect()
	end


	end

	end

	StarterGui:SetCore("SendNotification", {

		Title = "Anti Stun",

		Text = "Toggle On or Off",



		Duration = 50,

		Callback = Callback,

		Button1 = "ON",
		Button2 = "OFF"
	})
	end)
tab1:Button("Always Show inventory",function(state)
	local sound = Instance.new("Sound")
	sound.SoundId = "rbxassetid://1673280232"
	sound.Parent = game:GetService("SoundService")
	sound:Play()
	local StarterGui = game:GetService("StarterGui")

	local Args = Instance.new("BindableFunction")

	local player = game.Players.LocalPlayer


	function Args.OnInvoke(response)

	if response == "ON" then
	local sound = Instance.new("Sound")
	sound.SoundId = "rbxassetid://1673280232"
	sound.Parent = game:GetService("SoundService")
	sound:Play()

	if always_show_inventory then
	always_show_inventory:Disconnect()
	end
	local function always_show_inventory_Fuc()
	wait(0.5)
	if game.Players.LocalPlayer.Character.Humanoid.WalkSpeed == 0 then
	game:GetService("StarterGui"):SetCoreGuiEnabled('Backpack', true)

	end
	end
	always_show_inventory = game:GetService("RunService").RenderStepped:Connect(always_show_inventory_Fuc)

	else
		local sound = Instance.new("Sound")
	sound.SoundId = "rbxassetid://1673280232"
	sound.Parent = game:GetService("SoundService")
	sound:Play()

	if always_show_inventory then
	always_show_inventory:Disconnect()
	end


	end

	end

	StarterGui:SetCore("SendNotification", {

		Title = "Always Show Inventory",

		Text = "Toggle On or off",



		Duration = 50,

		Callback = Args,
		Button1 = "ON",
		Button2 = "OFF"
	})


	end)
wait(0.5)
tab1:Button("Anti Slow", function()
	local sound = Instance.new("Sound")
	sound.SoundId = "rbxassetid://1673280232"
	sound.Parent = game:GetService("SoundService")
	sound:Play()
	local StarterGui = game:GetService("StarterGui")

	local Args = Instance.new("BindableFunction")

	local player = game.Players.LocalPlayer


	function Args.OnInvoke(response)

	if response == "ON" then
	local sound = Instance.new("Sound")
	sound.SoundId = "rbxassetid://1673280232"
	sound.Parent = game:GetService("SoundService")
	sound:Play()

	if anti_slow then
	anti_slow:Disconnect()
	end
	local function anti_slow_Fuc()
	if game.Players.LocalPlayer.Character.Humanoid.WalkSpeed == 5 then
	game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 19
	game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
	end
	end
	anti_slow = game:GetService("RunService").RenderStepped:Connect(anti_slow_Fuc)

	else
		local sound = Instance.new("Sound")
	sound.SoundId = "rbxassetid://1673280232"
	sound.Parent = game:GetService("SoundService")
	sound:Play()

	if anti_slow then
	anti_slow:Disconnect()
	end


	end

	end

	StarterGui:SetCore("SendNotification", {

		Title = "Anti Slow",

		Text = "Toggle On or Off",



		Duration = 50,

		Callback = Args,
		Button1 = "ON",
		Button2 = "OFF"
	})


	end)
