tab1:Button("Obtain Ice Potion",function()
	local sound = Instance.new("Sound")
	sound.SoundId = "rbxassetid://1673280232"
	sound.Parent = game:GetService("SoundService")
	sound:Play()
	for i,v in pairs(game:GetService("Workspace").Map.System:GetDescendants()) do
	if v.Name == ("Big Freezer") then
	speaker.Character:FindFirstChildOfClass("Humanoid").Sit = true

	local currentX = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.X
	local currentY = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.Y
	local currentZ = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.Z
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Map.System["Big Freezer"].FreezeModel.HitBox.CFrame
	wait(0.3)
	game.Players.localPlayer.Character:FindFirstChildOfClass("Humanoid").Jump = true

	for _,no3 in pairs(game:GetService("Workspace").Map.System["Big Freezer"].FreezeModel:GetDescendants()) do
	if no3:IsA("ClickDetector") then
	fireclickdetector(no3)
	end
	end

	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(currentX, currentY, currentZ)
	wait(0.1)
	game.Players.localPlayer.Backpack:FindFirstChild("Ice Potion").Name = "Zawardo Potion"
	wait(0.1)
	game.Players.localPlayer.Backpack:FindFirstChild("Zawardo Potion").ToolTip = "JoJo A Reference!"

	elseif v.Name == ("Freezer") then
	speaker.Character:FindFirstChildOfClass("Humanoid").Sit = true
	local currentX = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.X
	local currentY = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.Y
	local currentZ = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.Z

	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Map.System.Freezer.FreezeModel.Handle.CFrame
	wait(0.3)
	game.Players.localPlayer.Character:FindFirstChildOfClass("Humanoid").Jump = true


	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(currentX, currentY, currentZ)
	wait(0.1)
	game.Players.localPlayer.Backpack:FindFirstChild("Ice Potion").Name = "Zawardo Potion"
	wait(0.1)
	game.Players.localPlayer.Backpack:FindFirstChild("Zawardo Potion").ToolTip = "JoJo A Reference!"


	elseif v.Name == ("Refrigerator Hallway") then
	speaker.Character:FindFirstChildOfClass("Humanoid").Sit = true
	local currentX = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.X
	local currentY = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.Y
	local currentZ = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.Z

	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game:GetService("Workspace").Map.System["Refrigerator Hallway"].FreezeModel.HitBox.CFrame
	wait(0.3)
	game.Players.localPlayer.Character:FindFirstChildOfClass("Humanoid").Jump = true

	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(currentX, currentY, currentZ)
	wait(0.3)
	game.Players.localPlayer.Backpack:FindFirstChild("Ice Potion").Name = "Zawardo Potion"
	wait(0.3)
	game.Players.localPlayer.Backpack:FindFirstChild("Zawardo Potion").ToolTip = "JoJo A Reference!"


	end
	end
	end)
