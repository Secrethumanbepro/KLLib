Reachv2.MouseButton1Up:Connect(function()
	local sound = Instance.new("Sound")
	sound.SoundId = "rbxassetid://413861777"
	sound.Parent = game:GetService("SoundService")
	sound:Play()
	local mm2 = game.Players.LocalPlayer.Backpack
	local speaker = game.Players.LocalPlayer
	for i,e in pairs(speaker:FindFirstChildOfClass("Backpack"):GetChildren()) do
	if e.Name == "Bat" or e.Name == "Katana" or e.Name == "Bottle" or e.Name == "Chain" then
	e.Parent = speaker.Character
	end
	end
	for i,v in pairs(game:GetService'Players'.LocalPlayer.Character:GetChildren()) do
	if v.Name == "Bat" or v.Name == "Katana" then

	v.BodyAttach.Massless = true
	v.BodyAttach.Transparency = 0.75
	v.BodyAttach.Size = Vector3.new(8, 5, 13)
	local selectionBox = Instance.new("SelectionBox",v.BodyAttach)
	selectionBox.Adornee = v.BodyAttach
	selectionBox.Transparency = 0.9
	speaker.Character:FindFirstChildOfClass('Humanoid'):UnequipTools()
	else
		for i,mmz in pairs(game:GetService'Players'.LocalPlayer.Character:GetChildren()) do

	if mmz.Name == "Bottle" then
	speaker.Character:FindFirstChildOfClass('Humanoid'):UnequipTools()

	mm2.Bottle.Handle.Massless = true
	mm2.Bottle.Handle.Transparency = 0.75
	mm2.Bottle.Handle.Size = Vector3.new(5, 10, 5)
	local selectionBox = Instance.new("SelectionBox",mm2.Bottle.Handle)
	selectionBox.Adornee = mm2.Bottle.Handle
	SurfaceSelection = 0
	selectionBox.Transparency = 0.9
	else
		for i,mmh in pairs(game:GetService'Players'.LocalPlayer.Character:GetChildren()) do
	if mmh.Name == "Chain" then
	speaker.Character:FindFirstChildOfClass('Humanoid'):UnequipTools()

	game.Players.localPlayer.Backpack.Chain.Cooldown.Value = 1.1
	game.Players.localPlayer.Backpack.Chain.WeaponAttach.Massless = true
	game.Players.localPlayer.Backpack.Chain.WeaponAttach.Size = Vector3.new(13, 5, 5)
	local selectionBox = Instance.new("SelectionBox",game.Players.localPlayer.Backpack.Chain.WeaponAttach)
	selectionBox.Adornee = game.Players.localPlayer.Backpack.Chain.WeaponAttach
	selectionBox.Transparency = 0.9



	end
	end
	end
	end
	end
	end

	end)
BringPlayer.Name = "BringPlayer"
BringPlayer.Parent = ScreenGui
BringPlayer.BackgroundColor3 = Color3.fromRGB(71, 71, 71)
BringPlayer.BackgroundTransparency = 0.7
BringPlayer.BorderSizePixel = 1
BringPlayer.Position = UDim2.new(0, 149, 0, -28)
BringPlayer.Size = UDim2.new(0, 80, 0, 30)
BringPlayer.Font = Enum.Font.SciFi
BringPlayer.Text = "BringPlayer"
BringPlayer.TextColor3 = Color3.fromRGB(255, 255, 255)
BringPlayer.TextScaled = true
BringPlayer.Draggable = false
BringPlayer.FocusLost:connect(function()
	for i,v in pairs(game.Players:GetChildren()) do
	if (string.sub(string.lower(v.Name),1,string.len(BringPlayer.Text))) == string.lower(BringPlayer.Text) then
	BringPlayer.Text = v.Name

	local currentX = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.X
	local currentY = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.Y
	local currentZ = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.Z


	Target = BringPlayer.Text
	NOW = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
	game.Players.LocalPlayer.Character.Humanoid.Name = 1
	local l = game.Players.LocalPlayer.Character["1"]:Clone()
	l.Parent = game.Players.LocalPlayer.Character
	l.Name = "Humanoid"
	wait(0.1)
	game.Players.LocalPlayer.Character["1"]:Destroy()
	game.Workspace.CurrentCamera.CameraSubject = game.Players.LocalPlayer.Character
	game.Players.LocalPlayer.Character.Animate.Disabled = true
	wait(0.1)
	game.Players.LocalPlayer.Character.Animate.Disabled = false
	game.Players.LocalPlayer.Character.Humanoid.DisplayDistanceType = "None"
	for i,v in pairs(game:GetService'Players'.LocalPlayer.Backpack:GetChildren())do
	game.Players.LocalPlayer.Character.Humanoid:EquipTool(v)
	end
	wait(0.5)
	local function tp(player,player2)
	local char1,char2 = player.Character,player2.Character
	if char1 and char2 then
	char1.HumanoidRootPart.CFrame = char2.HumanoidRootPart.CFrame
	end
	end
	local function getout(player,player2)
	local char1,char2 = player.Character,player2.Character
	if char1 and char2 then
	char1:MoveTo(char2.Head.Position)
	end
	end
	tp(game.Players[Target], game.Players.LocalPlayer)
	wait(0.1)
	tp(game.Players[Target], game.Players.LocalPlayer)
	wait(0.3)
	getout(game.Players.LocalPlayer, game.Players[Target])
	wait(1.5)
	wait(5.21)
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(currentX, currentY, currentZ)

	end
	end
end)
tab1:Section("IDK?")
tab1:Button("Hide Your Own Key",function()
	local sound = Instance.new("Sound")
	sound.SoundId = "rbxassetid://1673280232"
	sound.Parent = game:GetService("SoundService")
	sound:Play()
	local X = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.X
	local Y = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.Y
	local Z = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.Z

	game.Players.localPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-346.036, 12, -4.759)
	wait(0.6)
	game.Players.localPlayer.Character.Humanoid:TakeDamage(100)
	wait(5.25)
	game.Players.localPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(X, Y, Z)

	end)
