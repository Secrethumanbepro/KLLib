Reach.MouseButton1Up:Connect(function()
	local sound = Instance.new("Sound")
	sound.SoundId = "rbxassetid://413861777"
	sound.Parent = game:GetService("SoundService")
	sound:Play()
	for i,e in pairs(speaker:FindFirstChildOfClass("Backpack"):GetChildren()) do
	if e.Name == "Branch" or e.Name == "Bottle" or e.Name == "Packed Ice" or e.Name == "Spear" then
	e.Parent = speaker.Character
	end
	end

	for i,v in pairs(game:GetService'Players'.LocalPlayer.Character:GetChildren()) do
	if v.Name == "Bottle" or v.Name == "Packed Ice" or v.Name == "Branch" then
    
	v.Handle.Massless = true
	v.Handle.Transparency = 0.75
	v.Handle.Size = Vector3.new(5, 10, 5)
	local selectionBox = Instance.new("SelectionBox",v.Handle)
	selectionBox.Adornee = v.Handle
	SurfaceSelection = 0
	selectionBox.Transparency = 0.9
	speaker.Character:FindFirstChildOfClass('Humanoid'):UnequipTools()
 	elseif v.Name == "Spear" then



	speaker.Character:FindFirstChildOfClass('Humanoid'):UnequipTools()


	game.Players.localPlayer.Backpack.Spear.Cooldown.Value = 0.48
	game.Players.localPlayer.Backpack.Spear.Hitbox.Massless = true
	game.Players.localPlayer.Backpack.Spear.Hitbox.Size = Vector3.new(5, 9, 5)
	local selectionBox = Instance.new("SelectionBox",game.Players.localPlayer.Backpack.Spear.Hitbox)
	selectionBox.Adornee = game.Players.localPlayer.Backpack.Spear.Hitbox
	selectionBox.Transparency = 0.9



	end
	end

	end)
