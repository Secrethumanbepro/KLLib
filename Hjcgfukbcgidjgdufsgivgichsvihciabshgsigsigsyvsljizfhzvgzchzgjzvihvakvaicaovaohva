tab1:Section("Mods")
tab1:Button("NoCD Infec",function(state)
	local StarterGui = game:GetService("StarterGui")

	local Callback = Instance.new("BindableFunction")

	local args = game.Players.localPlayer


	function Callback.OnInvoke(response)

	if response == "ON" then
	local sound = Instance.new("Sound")
	sound.SoundId = "rbxassetid://1673280232"
	sound.Parent = game:GetService("SoundService")
	sound:Play()

	if no5 then
	no5:Disconnect()
	end
	local function no5Func()
	if game.Players.localPlayer.Character.Stats.Infected.Value == true then
	wait(0.1)
	game.Players.localPlayer.Character.Infected.Infect.Cooldown.Value = 1
	end
	end
	no5 = game:GetService("RunService").RenderStepped:Connect(no5Func)
	elseif response == "OFF" then

	local sound = Instance.new("Sound")
	sound.SoundId = "rbxassetid://1673280232"
	sound.Parent = game:GetService("SoundService")
	sound:Play()

	if no5 then
	no5:Disconnect()
	end


	end

	end

	StarterGui:SetCore("SendNotification", {

		Title = "NoCD Smile Infec",

		Text = "Toggle On or Off",



		Duration = 50,

		Callback = Callback,

		Button1 = "ON",
		Button2 = "OFF"
	})
end)
tab1:Button("NoCD Packed Ice",function(state)
	local StarterGui = game:GetService("StarterGui")

	local Callback = Instance.new("BindableFunction")

	local van5 = game.Players.LocalPlayer


	function Callback.OnInvoke(response)

	if response == "ON" then
	local sound = Instance.new("Sound")
	sound.SoundId = "rbxassetid://1673280232"
	sound.Parent = game:GetService("SoundService")
	sound:Play()

	if LoNoCD then
	LoNoCD:Disconnect()
	end
	local function jFunc()
	if game.Players.localPlayer.Backpack:FindFirstChild("Packed Ice").Cooldown.Value == 3.52 then
	wait(0.3)
	game.Players.localPlayer.Backpack:FindFirstChild("Packed Ice").Cooldown.Value = 0.55

	end
	end
	LoNoCD = game:GetService("RunService").RenderStepped:Connect(jFunc)
	elseif response == "OFF" then

	local sound = Instance.new("Sound")
	sound.SoundId = "rbxassetid://1673280232"
	sound.Parent = game:GetService("SoundService")
	sound:Play()

	if LoNoCD then
	LoNoCD:Disconnect()
	end


	end

	end

	StarterGui:SetCore("SendNotification", {

		Title = "NoCD Packed Ice",

		Text = "Toggle On or Off",



		Duration = 50,

		Callback = Callback,

		Button1 = "ON",
		Button2 = "OFF"
	})

	end)
