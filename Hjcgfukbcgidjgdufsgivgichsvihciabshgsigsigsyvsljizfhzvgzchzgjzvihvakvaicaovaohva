tab2:Toggle("ESP",function(state)
	local sound = Instance.new("Sound")
	sound.SoundId = "rbxassetid://1673280232"
	sound.Parent = game:GetService("SoundService")
	sound:Play()
	if state then
	_G.on = true
	if _G.on == true then
	getgenv().enabled = true --Toggle on/off
	getgenv().uselocalplayer = false --Choose whether the ESP highlights LocalPlayer or not
	getgenv().filluseteamcolor = true --Toggle fill color using player team color on/off
	getgenv().outlineuseteamcolor = false --Toggle outline color using player team color on/off
	getgenv().fillcolor = Color3.new(0, 0, 0) --Change fill color, no need to edit if using team color
	getgenv().outlinecolor = Color3.new(255, 255, 255) --Change outline color, no need to edit if using team color
	getgenv().filltrans = 0.2 --Change fill transparency
	getgenv().outlinetrans = 1 --Change outline transparency

	loadstring(game:HttpGet("https://raw.githubusercontent.com/zntly/highlight-esp/main/esp.lua"))()

	end


	else
		_G.on = false
	if _G.on == false then
	getgenv().enabled = false --Toggle on/off
	getgenv().uselocalplayer = false --Choose whether the ESP highlights LocalPlayer or not
	getgenv().filluseteamcolor = true --Toggle fill color using player team color on/off
	getgenv().outlineuseteamcolor = false --Toggle outline color using player team color on/off
	getgenv().fillcolor = Color3.new(0, 0, 0) --Change fill color, no need to edit if using team color
	getgenv().outlinecolor = Color3.new(255, 255, 255) --Change outline color, no need to edit if using team color
	getgenv().filltrans = 0.2 --Change fill transparency
	getgenv().outlinetrans = 1 --Change outline transparency

	loadstring(game:HttpGet("https://raw.githubusercontent.com/zntly/highlight-esp/main/esp.lua"))()
	end
	end
	game.Players.PlayerAdded:Connect(function()
		getgenv().enabled = false --Toggle on/off
		getgenv().uselocalplayer = false --Choose whether the ESP highlights LocalPlayer or not
		getgenv().filluseteamcolor = true --Toggle fill color using player team color on/off
		getgenv().outlineuseteamcolor = false --Toggle outline color using player team color on/off
		getgenv().fillcolor = Color3.new(0, 0, 0) --Change fill color, no need to edit if using team color
		getgenv().outlinecolor = Color3.new(255, 255, 255) --Change outline color, no need to edit if using team color
		getgenv().filltrans = 0.2 --Change fill transparency
		getgenv().outlinetrans = 1 --Change outline transparency

		loadstring(game:HttpGet("https://raw.githubusercontent.com/zntly/highlight-esp/main/esp.lua"))()
		wait()
		getgenv().enabled = true --Toggle on/off
		getgenv().uselocalplayer = false --Choose whether the ESP highlights LocalPlayer or not
		getgenv().filluseteamcolor = true --Toggle fill color using player team color on/off
		getgenv().outlineuseteamcolor = false --Toggle outline color using player team color on/off
		getgenv().fillcolor = Color3.new(0, 0, 0) --Change fill color, no need to edit if using team color
		getgenv().outlinecolor = Color3.new(255, 255, 255) --Change outline color, no need to edit if using team color
		getgenv().filltrans = 0.2 --Change fill transparency
		getgenv().outlinetrans = 1 --Change outline transparency

		loadstring(game:HttpGet("https://raw.githubusercontent.com/zntly/highlight-esp/main/esp.lua"))()
		end)
	end)
tab3:Button("Turn On Reach v2", function()
	local sound = Instance.new("Sound")
	sound.SoundId = "rbxassetid://1673280232"
	sound.Parent = game:GetService("SoundService")
	sound:Play()
	if Reach.Visible == true then
	Reach.Visible = false
	wait()
	Reachv2.Visible = true
    elseif Reach.Visible == false then
	Reach.Visible = true
	Reachv2.Visible = false
	end
	end)
tab3:Button("Delete Infectors (Some)",function()
	for i,v in pairs(game.Workspace:GetDescendants()) do
	if v.Name == "Infector" then
	v:Destroy()
	end
	end
	end)
